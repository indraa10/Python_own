##program : 
from sys import argv
l1=[]
add=0
for i in range(1,len(argv)):
    print(argv[i])
    add += int(argv[i])
    l1.append(argv[i])
print("print the list :",l1,end='')
print(" and print the sum : %i" %add )

## output :
#1
#2
#3
#4
#5
##print the list : ['1', '2', '3', '4', '5'] and print the sum : 15
